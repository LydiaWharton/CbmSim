####### Compiler, tools and options

CXX           = g++
CXXFLAGS      = -m64 -pipe -std=c++11 -O2 -fPIC 
INCPATH       = -I. -I../CXX_TOOLS_LIB/include -I../CBM_CORE_LIB/include -Iinclude
LINK          = g++
LFLAGS        = -m64 -Wl,-O1 -shared 

CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p
RMDIR         = rmdir

####### Output directory

OBJECTS_DIR   = objs/
INC_DIR       = include/
SRC_DIR       = src/


####### Files

SOURCES       = $(SRC_DIR)ecmfpopulation.cpp \
		$(SRC_DIR)eyelidintegrator.cpp \
		$(SRC_DIR)poissonregencells.cpp 

OBJECTS       = $(OBJECTS_DIR)ecmfpopulation.o \
		$(OBJECTS_DIR)eyelidintegrator.o \
		$(OBJECTS_DIR)poissonregencells.o

first: $(OBJECTS_DIR) all


####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"


####### Build rules

$(OBJECTS_DIR):
	@$(CHK_DIR_EXISTS) $(OBJECTS_DIR) || $(MKDIR) $(OBJECTS_DIR)

all: $(OBJECTS)


####### Compile

$(OBJECTS_DIR)ecmfpopulation.o: $(SRC_DIR)ecmfpopulation.cpp $(INC_DIR)ecmfpopulation.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)ecmfpopulation.o $(SRC_DIR)ecmfpopulation.cpp

$(OBJECTS_DIR)eyelidintegrator.o: $(SRC_DIR)eyelidintegrator.cpp $(INC_DIR)eyelidintegrator.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)eyelidintegrator.o $(SRC_DIR)eyelidintegrator.cpp

$(OBJECTS_DIR)poissonregencells.o: $(SRC_DIR)poissonregencells.cpp $(INC_DIR)poissonregencells.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)poissonregencells.o $(SRC_DIR)poissonregencells.cpp


####### House-cleaning

clean:
	rm -rf $(OBJECTS)
	$(RMDIR) $(OBJECTS_DIR)

check: first

