####### Compile non-cuda sources

$(OBJECTS_DIR)innet.o: $(SRC_DIR)innetmodules/innet.cpp $(INC_DIR)/innetmodules/innet.h \
		$(INC_DIR)cuda/kernels.h \
		$(INC_DIR)interface/innetinterface.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)innet.o $(SRC_DIR)innetmodules/innet.cpp

$(OBJECTS_DIR)innetallgrmfgo.o: $(SRC_DIR)innetmodules/innetallgrmfgo.cpp $(INC_DIR)innetmodules/innetallgrmfgo.h \
		$(INC_DIR)innetmodules/innet.h \
		$(INC_DIR)cuda/kernels.h \
		$(INC_DIR)interface/innetinterface.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)innetallgrmfgo.o $(SRC_DIR)innetmodules/innetallgrmfgo.cpp

$(OBJECTS_DIR)cbmsimcore.o: $(SRC_DIR)interface/cbmsimcore.cpp $(INC_DIR)interface/cbmsimcore.h \
		$(INC_DIR)interface/innetinterface.h \
		$(INC_DIR)interface/mzoneinterface.h \
		$(INC_DIR)mzonemodules/mzone.h \
		$(INC_DIR)cuda/kernels.h \
		$(INC_DIR)innetmodules/innet.h \
		$(INC_DIR)innetmodules/innetallgrmfgo.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)cbmsimcore.o $(SRC_DIR)interface/cbmsimcore.cpp

$(OBJECTS_DIR)cbmsimx2grgodecouple.o: $(SRC_DIR)interface/cbmsimx2grgodecouple.cpp $(INC_DIR)interface/cbmsimx2grgodecouple.h \
		$(INC_DIR)interface/innetinterface.h \
