		$(INC_DIR)innetmodules/innet.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)cbmsimx2grgodecouple.o $(SRC_DIR)interface/cbmsimx2grgodecouple.cpp

$(OBJECTS_DIR)innetinterface.o: $(SRC_DIR)interface/innetinterface.cpp $(INC_DIR)interface/innetinterface.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)innetinterface.o $(SRC_DIR)interface/innetinterface.cpp

$(OBJECTS_DIR)mzoneinterface.o: $(SRC_DIR)interface/mzoneinterface.cpp $(INC_DIR)interface/mzoneinterface.h
