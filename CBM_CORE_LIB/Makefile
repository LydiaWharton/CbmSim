####### House-cleaning

clean: compiler_cuda_clean
	rm -rf $(OBJECTS)
	$(RMDIR) $(OBJECTS_DIR)

compiler_cuda_clean:
	rm -rf $(OBJECTS_DIR)kernels_cuda.o

