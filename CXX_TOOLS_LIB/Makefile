####### Compiler, tools and options

CXX           = g++
CXXFLAGS      = -m64 -pipe -O2 -std=c++11 -fPIC 
INCPATH       = -I. -Iinclude 
LINK          = g++
LFLAGS        = -m64 -Wl,-O1 -shared 

CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p
RMDIR         = rmdir 

####### Output directory

OBJECTS_DIR   = objs/
INC_DIR       = include/
SRC_DIR       = src/


####### Files

SOURCES       = $(SRC_DIR)fileIO/rawbytesrw.cpp \
		$(SRC_DIR)memoryMgmt/arrayvalidate.cpp \
		$(SRC_DIR)memoryMgmt/dynamic2darray.cpp \
		$(SRC_DIR)randGenerators/mersenne.cpp \
		$(SRC_DIR)randGenerators/mother.cpp \
		$(SRC_DIR)randGenerators/sfmt.cpp 

OBJECTS       = $(OBJECTS_DIR)rawbytesrw.o \
		$(OBJECTS_DIR)arrayvalidate.o \
		$(OBJECTS_DIR)dynamic2darray.o \
		$(OBJECTS_DIR)mersenne.o \
		$(OBJECTS_DIR)mother.o \
		$(OBJECTS_DIR)sfmt.o

first: $(OBJECTS_DIR) all


####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"


####### Build rules

$(OBJECTS_DIR):
	@$(CHK_DIR_EXISTS) $(OBJECTS_DIR) || $(MKDIR) $(OBJECTS_DIR)

all: $(OBJECTS)


####### Compile

$(OBJECTS_DIR)rawbytesrw.o: $(SRC_DIR)fileIO/rawbytesrw.cpp $(INC_DIR)fileIO/rawbytesrw.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)rawbytesrw.o $(SRC_DIR)fileIO/rawbytesrw.cpp

$(OBJECTS_DIR)arrayvalidate.o: $(SRC_DIR)memoryMgmt/arrayvalidate.cpp $(INC_DIR)memoryMgmt/arrayvalidate.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)arrayvalidate.o $(SRC_DIR)memoryMgmt/arrayvalidate.cpp

$(OBJECTS_DIR)dynamic2darray.o: $(SRC_DIR)memoryMgmt/dynamic2darray.cpp $(INC_DIR)memoryMgmt/dynamic2darray.h \
		$(INC_DIR)memoryMgmt/arrayvalidate.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)dynamic2darray.o $(SRC_DIR)memoryMgmt/dynamic2darray.cpp

$(OBJECTS_DIR)mersenne.o: $(SRC_DIR)randGenerators/mersenne.cpp $(INC_DIR)randGenerators/randomc.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)mersenne.o $(SRC_DIR)randGenerators/mersenne.cpp

$(OBJECTS_DIR)mother.o: $(SRC_DIR)randGenerators/mother.cpp $(INC_DIR)randGenerators/randomc.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)mother.o $(SRC_DIR)randGenerators/mother.cpp

$(OBJECTS_DIR)sfmt.o: $(SRC_DIR)randGenerators/sfmt.cpp $(INC_DIR)randGenerators/sfmt.h \
		$(INC_DIR)randGenerators/randomc.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $(OBJECTS_DIR)sfmt.o $(SRC_DIR)randGenerators/sfmt.cpp

####### House-keeping

clean:
